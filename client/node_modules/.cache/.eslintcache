  const [addPodcast, { error }] = useMutation(SAVE_PODCAST, {\n    update(cache, { data: { addPodcast } }) {\n      try {\n        const { podcasts } = cache.readQuery({ query: QUERY_PODCASTS });\n\n        cache.writeQuery({\n          query: QUERY_PODCASTS,\n          data: { podcasts: [addPodcast, ...podcasts] },\n        });\n      } catch (e) {\n        console.error(e);\n      }\n\n      // update user object's cache\n      const { user } = cache.readQuery({ query: QUERY_USER });\n      cache.writeQuery({\n        query: QUERY_USER,\n        data: { user: { ...user, podcasts: [...user.podcasts, addPodcast] } },\n      });\n    },\n  });\n\n  // Button Handler to Add Podcast to MyPodcasts\n  const handleButtonSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addPodcast({\n        variables: {\n          username: Auth.getProfile().data.username,\n        },\n      });\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n\n  const history = useHistory();\n  \n  return (\n    <div>\n      <Async promiseFn={loadPodcast}>\n        {({ data, err, isLoading }) => {\n          if (isLoading) return \"Loading...\";\n          if (err) return `Something went wrong: ${err.message}`;\n          if (data)\n            return (\n              <div>\n                <div className=\"title\">\n                  <h1>{data.title.split(' ').slice(0, 10).join(' ')}</h1>\n                  <a><div\n                    onClick={() => {\n                      history.goBack();\n                    }}\n                  >\n                    <h2>← Go Back</h2>\n                  </div></a>\n                </div>\n                <div onSubmit={handleButtonSubmit} className=\"podcast-info\">\n                  <div className=\"podcast-info-details\">\n                    <div className=\"podcast-info-cover\">\n                      <img src={data.image} alt={data.title}></img>\n                    </div>\n                    <div>\n                      <div className=\"podcast-info-text\" style={{\n                            paddingLeft: `0`,\n                          }}>\n                        <ul className=\"description\" style={{padding: \"0px\"}}>\n                          <li><span>Country:</span> {data.country}</li>\n                          <li><span>Language:</span> {data.language}</li>\n                          <li><span>Publisher:</span> {data.publisher}</li>\n                          <li><a href={data.website} target=\"_blank\" rel=\"noreferrer noopener\">Visit the official website</a></li>\n                        </ul>\n                      </div>\n                      <div className=\"like\">\n                        {/* <button className=\"heart\">\n                        <Logo className=\"icon\" />\n                        </button> */}\n                        <button type=\"submit\" className=\"button\">\n                          Add To My Podcasts\n                        </button>\n                                              </div>\n                    </div>\n                  </div>\n                  <div className=\"podcast-info-description\">\n                    <span>Description:</span>\n                  <p>{data.description}</p>\n                </div>\n                </div>\n\n                <div className=\"title\">\n                  <h1>Episodes</h1>\n                </div>\n                <div className=\"episodes-boxes\">\n                  {data.episodes.map((item) => (\n                    <a href={\"/podcast/\" + [item.id]}>\n                      <div className=\"box-section\">\n                        <div\n                          className=\"episode-cover\"\n                          style={{\n                            backgroundImage: `url(${item.image})`,\n                          }}\n                        ></div>\n                        {item.title}\n                      </div>\n                    </a>\n                  ))}\n                </div>\n                {/* <a href={\"/nextpodcastepisodes/\" + [data.id] + \"/\" + [data.next_episode_pub_date]}>Next Page</a> */}\n              </div>\n            );\n        }}\n      </Async>\n    </div>\n  );\n}\n\nexport default PodcastInfo;\n","/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/BestPodcastsList/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/PodcastItem/index.js",["149"],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/OneCategory/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/NewPodcastsList/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/utils/queries.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/CardSection/index.js",["150"],"/**\n* Use the CSS tab above to style your Element's container.\n*/\nimport React from 'react';\nimport {CardElement} from '@stripe/react-stripe-js';\nimport './CardSectionStyles.css';\nimport { Fragment } from 'react';\n\nconst CARD_ELEMENT_OPTIONS = {\n  style: {\n    base: {\n      color: \"#32325d\",\n      fontFamily: '\"Helvetica Neue\", Helvetica, sans-serif',\n      fontSmoothing: \"antialiased\",\n      fontSize: \"16px\",\n      \"::placeholder\": {\n        color: \"#aab7c4\",\n      },\n    },\n    invalid: {\n      color: \"#fa755a\",\n      iconColor: \"#fa755a\",\n    },\n  },\n};\n\nfunction CardSection() {\n  return (\n    <div className=\"donationSection\">\n        <label>Amount you want to donate</label>\n        <input type=\"text\" name=\"amount\"></input>\n        <label>Name on card</label>\n        <input type=\"text\" name=\"customerName\"></input>\n        <label>Your card details</label>\n        <CardElement options={CARD_ELEMENT_OPTIONS} />\n    </div> \n  );\n};\n\nexport def