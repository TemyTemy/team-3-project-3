       <SearchBar />\n        </div>\n      </section>\n\n      {/* <section>\n        <div className=\"title\">\n          <h1>Find a Podcast</h1>\n        </div>\n        <div className=\"form\">\n          <SearchFilters />\n        </div>\n      </section> */}\n\n    </div>\n  );\n}\n","/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/pages/Podcast.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/pages/PodcastEpisodes.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/pages/Donate.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/pages/Home.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/pages/Login.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/pages/Signup.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/Header/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/Nav/index.js",["140","141"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nimport { ReactComponent as HomeIcon } from \"../../assets/home.svg\";\nimport { ReactComponent as SearchIcon } from \"../../assets/search.svg\";\nimport { ReactComponent as SettingsIcon } from \"../../assets/user.svg\";\nimport { ReactComponent as MyPodcastsIcon } from \"../../assets/list.svg\";\nimport { ReactComponent as DonateIcon } from \"../../assets/donate.svg\";\n\nexport default class Nav extends React.Component {\n  render() {\n    return (\n      <footer>\n        <nav>\n          <ul>\n            {/* <li>\n              <Link to=\"/\">\n                <span>\n                  <HomeIcon className=\"icon\" />\n                </span>\n                <p>Home</p>\n              </Link>\n            </li> */}\n            <li>\n              <Link to=\"/search\">\n                <span>\n                  <SearchIcon className=\"icon\" />\n                </span>\n                <p>Search</p>\n              </Link>\n            </li>\n            {/* <li>\n              <Link to=\"/settings\">\n                <span>\n                  <SettingsIcon className=\"icon\" />\n                </span>\n                <p>Account</p>\n              </Link>\n            </li> */}\n            <li>\n              <Link to=\"/mypodcasts\">\n                <span>\n                  <MyPodcastsIcon className=\"icon\" />\n                </span>\n                <p>My Podcasts</p>\n              </Link>\n            </li>\n            <li>\n              <Link to=\"/donate\">\n                <span>\n                  <DonateIcon className=\"icon\" />\n                </span>\n                <p>Donate</p>\n              </Link>\n            </li>\n          </ul>\n        </nav>\n      </footer>\n    );\n  }\n}\n","/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/NextPodcasts/index.js",["142"],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/utils/mutations.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/utils/auth.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/MyPodcastsDisplay/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/CategoryList/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/OneCategory/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/SearchByFilters/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/BestPodcastsList/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/PodcastItem/index.js",["143"],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/SearchByWord/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/DonateForm/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/NewPodcastsList/index.js",[],"/Users/rsd/Documents/08_BOOTCAMP/23_Project_Three/team-3-project-3/client/src/components/PodcastList/index.js",["144","145","146","147"],"// import { iterateObserversSafely } from \"@apollo/client/utilities\";\nimport React from \"react\";\nimport Async from \"react-async\";\nimport { useHistory } from \"react-router-dom\";\nimport { useMutation } from '@apollo/client';\nimport { ReactComponent as Logo } from \"../../assets/like.svg\";\nimport { SAVE_PODCAST } from '../../utils/mutations';\nimport { QUERY_PODCASTS, QUERY_USER } from '../../utils/queries';\nimport Auth from '../../utils/auth';\n\n\n\n// We'll request podcasts episodes from this API\nconst ID = window.location.pathname.split(\"/\").pop();\nconst API_KEY = process.env.REACT_APP_API_KEY;\nconst URL = \"https://listen-api.listennotes.com/api/v2/podcasts/\" + ID;\nconst loadPodcast = () =>\n  fetch(URL, {\n    method: \"GET\",\n    headers: {\n      \"Content-type\": \"application/json;charset=UTF-8\",\n      \"X-ListenAPI-Key\": API_KEY,\n    },\n  })\n    .then((res) => (res.ok ? res : Promise.reject(res)))\n    .then((res) => res.json());\n\nfunction PodcastInfo() {\n\n// ADD PODCAST TO MYPODCASTS\n  const [addPodcast, { error }] = useMutation(SAVE_PODCAST, {\n    update(cache, { data: { addPodcast } }) {\n      try {\n        const { podcasts } = cache.readQuery({ query: QUERY_PODCASTS });\n\n        cache.writeQuery({\n          query: QUERY_PODCASTS,\n          data: { podcasts: [addPodcast, ...podcasts] },\n        });\n      } catch (e) {\n        console.error(e);\n      }\n\n      // update user object's cache\n      const { user } = cache.readQuery({ query: QUERY_USER });\n      cache.writeQuery({\n        query: QUERY_USER,\n        data: { user: { ...user, podcasts: [...user.podcasts, addPodcast] } },\n      });\n    },\n  });\n\n  // Button Handler to Add Podcast to MyPodcasts\n  const handleButtonSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addPodcast({\n        variables: {\n          username: Auth.getProfile().data.username,\n        },\n      });\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n\n  const history = useHistory();\n  \n  return (\n    <div>\n      <Async promiseFn={loadPodcast}>\n        {({ data, err, isLoading }) => {\n          if (isLoading) return \"Loading...\";\n          if (err) return `Something went wrong: ${err.message}`;\n          if (data)\n            return (\n              <div>\n                <div className=\"title\">\n                  <h1>{data.title.split(' ').slice(0, 10).join(' ')}</h1>\n                  <a><div\n                    onClick={() => {\n                      history.goBack();\n                    }}\n                  >\n                    <h2>← Go Back</h2>\n                  </div></a>\n                </div>\n                <div onSubmit={handleButtonSubmit} className=\"podcast-info\">\n                  <div className=\"podcast-info-details\">\n                    <div className=\"podcast-info-cover\">\n                      <img src={data.image} alt={data.title}></img>\n                    </div>\n                    <div>\n                      <div className=\"podcast-info-text\" style={{\n                            paddingLeft: `0`,\n                          }}>\n                        <ul className=\"description\" style={{padding: \"0px\"}}>\n                          <li><span>Country:</span> {data.country}</li>\n                          <li><span>Language:</span> {data.language}</li>\n                          <li><span>Publisher:</span> {data.publisher}</li>\n                          <li><a