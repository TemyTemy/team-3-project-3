ri: '/graphql',\n  cache: new InMemoryCache(),\n});\n\nexport default class App extends React.Component {\n  render() {\n    return (\n      <ApolloProvider client={client}>\n      <div>\n        <Router>\n          <div>\n            <Header />\n            <Nav />\n            <Elements stripe={stripePromise}>\n              <Route exact path=\"/\" component={Home} />\n              <Route exact path=\"/search\" component={Search} />\n              {/* <Route exact path=\"/settings\" component={Profile} /> */}\n              <Route exact path=\"/mypodcasts\" component={MyPodcasts} />\n              <Route exact path=\"/categories\" component={Categories} />\n              <Route exact path=\"/podcastepisodes\" component={PodcastEpisodes} />\n              <Route exact path=\"/nextpodcastepisodes/:id/:page\" component={NextPodcasts} />\n              <Route exact path=\"/podcast\" component={Podcast} />\n              <Route exact path=\"/donate\" component={Donate} />\n              <Route exact path=\"/login\" component={Login} />\n              <Route exact path=\"/signup\" component={Signup} />\n              <Route exact path=\"/category/:id\" component={Category} />\n              <Route exact path=\"/bestpodcasts\" component={BestPodcasts} />\n              <Route exact path=\"/newpodcasts\" component={NewPodcasts} />\n              <Route exact path=\"/podcastepisodes/:id\" component={PodcastEpisodes} />\n              <Route exact path=\"/podcast/:id\" component={Podcast} /> \n            </Elements>                 \n          </div>\n        </Router>\n      </div>\n      </ApolloProvider>\n    );\n  }\n}\n","/Users/thomaswilliams/CODING_BOOTCAMP/assignments/team-3-project-3/client/src/pages/Mypodcasts.js",[],"/Users/thomaswilliams/CODING_BOOTCAMP/assignments/team-3-project-3/client/src/pages/Search.js",["138"],"import React from \"react\";\nimport SearchBar from \"../components/SearchByWord\"\nimport SearchFilters from \"../components/SearchByFilters\"\